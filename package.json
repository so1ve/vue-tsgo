{
  "name": "vue-tsgo",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.30.1",
  "description": "Type checker for Vue SFCs with TypeScript 7 integration",
  "author": "KazariEX",
  "license": "MIT",
  "repository": "KazariEX/vue-tsgo",
  "keywords": [],
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "bin": {
    "vue-tsgo": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "prepack": "pnpm run build",
    "release": "bumpp --no-push -c \"release: v%s\"",
    "test": "vitest"
  },
  "peerDependencies": {
    "@typescript/native-preview": "*"
  },
  "dependencies": {
    "@vue/compiler-dom": "catalog:",
    "@vue/language-core": "catalog:",
    "@vue/shared": "catalog:",
    "clerc": "catalog:",
    "empathic": "catalog:",
    "mlly": "catalog:",
    "muggle-string": "catalog:",
    "oxc-parser": "catalog:",
    "oxc-resolver": "catalog:",
    "oxc-walker": "catalog:",
    "pathe": "catalog:",
    "picomatch": "catalog:",
    "tinyexec": "catalog:",
    "tinyglobby": "catalog:",
    "tsconfck": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/picomatch": "catalog:",
    "@typescript/native-preview": "catalog:",
    "@zinkawaii/eslint-config": "catalog:",
    "@zinkawaii/tsconfig": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "pkg-types": "catalog:",
    "tsdown": "catalog:",
    "vitest": "catalog:"
  }
}
