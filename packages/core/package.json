{
  "name": "vue-tsgo",
  "type": "module",
  "version": "0.1.1",
  "description": "Type checker for Vue SFCs with TypeScript 7 integration",
  "author": "KazariEX",
  "license": "MIT",
  "repository": "KazariEX/vue-tsgo",
  "keywords": [],
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "bin": {
    "vue-tsgo": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "prepack": "pnpm run build",
    "test": "vitest",
    "generate-names": "unrun scripts/generate-names.ts"
  },
  "peerDependencies": {
    "@typescript/native-preview": "*"
  },
  "dependencies": {
    "@vue/compiler-dom": "catalog:",
    "@vue/language-core": "catalog:",
    "@vue/shared": "catalog:",
    "clerc": "catalog:",
    "empathic": "catalog:",
    "mlly": "catalog:",
    "muggle-string": "catalog:",
    "oxc-parser": "catalog:",
    "oxc-resolver": "catalog:",
    "oxc-walker": "catalog:",
    "pathe": "catalog:",
    "picomatch": "catalog:",
    "tinyexec": "catalog:",
    "tinyglobby": "catalog:",
    "tsconfck": "catalog:",
    "vscode-jsonrpc": "catalog:"
  },
  "devDependencies": {
    "@types/picomatch": "catalog:",
    "@typescript/native-preview": "catalog:",
    "pkg-types": "catalog:",
    "vscode-languageserver-protocol": "catalog:"
  }
}
